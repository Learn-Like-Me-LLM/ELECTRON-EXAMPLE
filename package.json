{
  "name": "electron_example",
  "version": "0.0.0",
  "main": "./dist-electron/main/index.js",
  "type": "commonjs",
  "scripts": {
    "drizzle:generateSchema": "node src/scripts/migrate.js generate",
    "drizzle:runMigrations": "node src/scripts/runMigrations.js",
    "drizzle:rebuild": "node src/scripts/migrate.js rebuild",
    "db:setup": "npm run drizzle:generateSchema && npm run drizzle:rebuild && npm run drizzle:runMigrations",
    "tanstackrouter:generate:routes": "npx @tanstack/router-cli ./src/routes -y",
    "dev": "vite",
    "dev:renderer": "cross-env RENDERER_ONLY_MODE=true vite",
    "debug": "concurrently --kill-others-on-fail \"npm run dev:renderer\" \"wait-on -t 120000 http://localhost:5173 && echo 'Vite dev server is ready on http://localhost:5173. You can now launch Debug MAIN PROCESS in VSCode.'\"",
    "build:main": "cross-env ELECTRON_MAIN=1 NODE_ENV=production vite build",
    "build:main:debug": "cross-env ELECTRON_MAIN=1 VSCODE_DEBUG=true NODE_ENV=development vite build -c vite.debug.config.ts",
    "build:renderer": "cross-env NODE_ENV=production vite build",
    "electron:build": "npm run build:renderer && npm run build:main && electron-builder --mac",
    "electron:rebuild": "rm -rf dist dist-electron release && cross-env NODE_ENV=production npm run electron:build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.76.0",
    "@tanstack/react-router": "^1.120.3",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^11.10.0",
    "classnames": "^2.5.1",
    "concurrently": "^9.1.2",
    "drizzle-orm": "^0.43.1",
    "moment": "^2.30.1",
    "path-browserify": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "short-uuid": "^4.2.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.76.0",
    "@tanstack/react-router-devtools": "^1.120.3",
    "@tanstack/router-cli": "^1.120.3",
    "@tanstack/router-devtools": "^1.120.3",
    "@tanstack/router-plugin": "^1.120.3",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.1",
    "electron": "^36.2.0",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.4.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^8.0.3"
  }
}
